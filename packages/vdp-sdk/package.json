{
  "name": "@instill-ai/vdp-sdk",
  "version": "0.0.4",
  "description": "Instill AI's Typescript SDK",
  "repository": "https://github.com/instill-ai/design-system/tree/main/packages/toolkit/src/lib/vdp-sdk",
  "bugs": "https://github.com/instill-ai/design-system/issues",
  "keywords": [
    "instill-ai",
    "SDK",
    "Typescript"
  ],
  "scripts": {
    "test": "vitest --run --passWithNoTests",
    "build": "pnpm rimraf dist && tsup src && pnpm ts-types",
    "ts-types": "tsc --emitDeclarationOnly",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --cache",
    "publish-rc": "pnpm build && pnpm publish --no-git-checks"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "private": false,
  "devDependencies": {
    "@instill-ai/eslint-config-cortex": "workspace:*",
    "@testing-library/jest-dom": "^5.16.5",
    "@vitejs/plugin-react": "^3.1.0",
    "esbuild": "^0.14.34",
    "eslint": "^8.35.0",
    "rimraf": "^3.0.2",
    "tsup": "^6.7.0",
    "tsx": "^3.12.3",
    "typescript": "^5.1.0",
    "vitest": "^0.29.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@instill-ai/toolkit": "workspace:*",
    "axios": "^1.3.4"
  },
  "peerDependencies": {
    "@instill-ai/toolkit": "workspace:*"
  }
}
